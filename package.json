{
  "name": "SVF",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios-open": "open ios/*.xcodeproj",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "dev-ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "dev-android": "node node_modules/react-native/local-cli/cli.js run-android",
    "lint": "eslint src test *.js",
    "test": "mocha --recursive --compilers js:babel-core/register -r test/setup",
    "cover": "babel-node node_modules/.bin/isparta cover _mocha -- --recursive -r test/setup",
    "remotedev": "remotedev-debugger --hostname localhost --port 5678 --injectserver",
    "postinstall": "node necard-patch.js && rnpm link && npm run remotedev && rndebugger-open",
    "author": "git log --format='%aN <%aE>' | sort -u  > AUTHORS.txt",
    "bundle-ios": "node node_modules/react-native/local-cli/cli.js bundle --entry-file index.ios.js  --platform ios --dev false --bundle-output ./ios/SVF/index.ios.jsbundle --assets-dest ./ios/SVF",
    "bundle-web": "NODE_ENV=production webpack -p --config web/webpack.config.js --colors",
    "dev-web": "node web/start-dev-server",
    "web": "node web/start-dev-server"
  },
  "dependencies": {
    "gn-react-native-carousel-control": "^1.0.6",
