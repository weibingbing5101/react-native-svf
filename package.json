    "react-native-autoheight-webview": "^0.2.1",
    "react-native-check-box": "^1.0.4",
    "react-native-drawer-layout": "^1.0.0",
    "react-native-easy-grid": "^0.1.8",
    "react-native-keyboard-spacer": "^0.3.1",
    "react-native-modal-datetime-picker":"^4.4.0",
    "react-native-nav": "^1.1.4",
    "react-native-root-toast": "^1.0.3",
    "react-native-storage": "^0.1.3",
    "react-native-swiper": "^1.4.8",
    "react-native-tab-navigator": "^0.3.3",
    "react-native-vector-icons": "^3.0.0",
    "react-redux": "^4.4.6",
    "react-web": "^0.2.4",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.3.15",
    "babel-eslint": "^6.0.0",
